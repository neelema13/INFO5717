   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The output for volume of the sphere with the respective radius is as follows:\n",
      "r   V\n",
      "1   4.1866666666666665\n",
      "2   33.49333333333333\n",
      "3   113.03999999999998\n",
      "4   267.94666666666666\n",
      "5   523.3333333333334\n",
      "6   904.3199999999998\n",
      "7   1436.0266666666666\n",
      "8   2143.5733333333333\n",
      "9   3052.08\n",
      "10   4186.666666666667\n"
     ]
    }
   ],
   "source": [
    "def calculate_vol_sphere(r):\n",
    "    return(4/3*3.14*r*r*r)\n",
    "print(\"The output for volume of the sphere with the respective radius is as follows:\")\n",
    "print(\"r\",\" \",\"V\")\n",
    "\n",
    "#for loop for generating the 10 volumes\n",
    "for x in range(10):\n",
    "    v=calculate_vol_sphere(x+1)\n",
    "    print(x+1,\" \",v)"
   ]
