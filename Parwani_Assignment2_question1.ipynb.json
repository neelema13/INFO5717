   "source": [
    "def calculate_vol_sphere(r):\n",
    "    return(4/3*3.14*r*r*r)\n",
    "print(\"The output for volume of the sphere with the respective radius is as follows:\")\n",
    "print(\"r\",\" \",\"V\")\n",
    "\n",
    "#for loop for generating the 10 volumes\n",
    "for x in range(10):\n",
    "    v=calculate_vol_sphere(x+1)\n",
    "    print(x+1,\" \",v)"
   ]
